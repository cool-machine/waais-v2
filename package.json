{
  "name": "wharton-ai-studio",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "description": "Wharton Alumni AI Studio and Research Center website",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests specified\" && exit 0",
    "clean": "rm -rf dist node_modules/.vite",
    "setup": "npm install && npm run type-check",
    "dev:full": "concurrently \"npm run dev\" \"cd server && npm run dev\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "lucide-react": "^0.263.1",
    "framer-motion": "^10.12.16",
    "react-toastify": "^9.1.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/node": "^20.4.2",
    "@vitejs/plugin-react": "^4.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "tailwindcss": "^3.3.3",
    "postcss": "^8.4.27",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "prettier": "^3.1.0",
    "concurrently": "^8.2.0"
  },
  "keywords": [
    "wharton",
    "ai",
    "studio",
    "alumni",
    "artificial-intelligence"
  ],
  "author": "Wharton Alumni AI Studio and Research Center",
  "license": "MIT"
}